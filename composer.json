    "type": "library",
    "license": "GPL-2.0-only",
    "authors": [{
            "name": "Bruno Santana",
            "email": "bruno.santana@f5consultoria.info"
        },
        {
            "name": "Luiz Henrique Cavalcante de Oliveira",
            "email": "luiz.henrique@f5consultoria.info"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.3",
        "ramsey/uuid": "^4.2",
        "chillerlan/php-qrcode": "^4.3",
        "nesbot/carbon": "^2.51"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
